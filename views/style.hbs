<?xml version="1.0" encoding="utf-8"?><!DOCTYPE Map> 
<Map bgcolor="transparent" srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0 +k=1.0 +units=m +nadgrids=@null +no_defs +over"> 
    <Stylesheet><![CDATA[
    {{#rules}}
    {{selector}} {
      {{#properties}}
        {{property}}: {{value}};
      {{/properties}}
    }
    {{/rules}}
    ]]></Stylesheet> 
  {{#layers}}<Layer id="{{id}}" name="data" srs="+proj=latlong +datum=WGS84"> 
    <Datasource> 
      <Parameter name="file">{{file}}</Parameter> 
      <Parameter name="type">{{type}}</Parameter> 
      <Parameter name="id">{{id}}</Parameter> 
    </Datasource> 
  </Layer>{{/layers}}
</Map> 
